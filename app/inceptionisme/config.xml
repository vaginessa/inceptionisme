<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="30" id="ch.scydev.inceptionisme.full" version="1.3.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Deep Dream Photo Filter</name>
    <description>
        The app to deep dream your images with a neural network.
    </description>
    <author email="lukas.saegesser@scydev.ch" href="http://www.scydev.ch">
        ScyDev GmbH
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <framework src="com.google.android.gms:play-services-plus:+" />
    <framework src="com.google.android.gms:play-services-identity:+" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="local" />
    </platform>
    <plugin name="cordova-plugin-googleplayservices" spec="~19.0.3" />
    <plugin name="cordova-plugin-crop" spec="~0.3.1" />
</widget>
